  number = {1},
  journal = {International Review of Sport and Exercise Psychology},
  author = {Hynynen, Sini-Tuuli and {van Stralen}, M. M. and Sniehotta, F. F. and Ara{\'u}jo-Soares, V. and Hardeman, W. and Chinapaw, M. J. M. and Vasankari, T. and Hankonen, Nelli},
  month = jan,
