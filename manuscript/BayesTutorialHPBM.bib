@article{schonbrodt_bayes_2017,
  title = {Bayes Factor Design Analysis: {{Planning}} for Compelling Evidence},
  issn = {1069-9384, 1531-5320},
  shorttitle = {Bayes Factor Design Analysis},
  doi = {10.3758/s13423-017-1230-y},
  abstract = {A sizeable literature exists on the use of frequentist power analysis in the null-hypothesis significance testing (NHST) paradigm to facilitate the design of informative experiments. In contrast, there is almost no literature that discusses the design of experiments when Bayes factors (BFs) are used as a measure of evidence. Here we explore Bayes Factor Design Analysis (BFDA) as a useful tool to design studies for maximum efficiency and informativeness. We elaborate on three possible BF designs, (a) a fixed-n design, (b) an open-ended Sequential Bayes Factor (SBF) design, where researchers can test after each participant and can stop data collection whenever there is strong evidence for either H1H1$\backslash$mathcal \{H\}\_\{1\} or H0H0$\backslash$mathcal \{H\}\_\{0\}, and (c) a modified SBF design that defines a maximal sample size where data collection is stopped regardless of the current state of evidence. We demonstrate how the properties of each design (i.e., expected strength of evidence, expected sample size, expected probability of misleading evidence, expected probability of weak evidence) can be evaluated using Monte Carlo simulations and equip researchers with the necessary information to compute their own Bayesian design analyses.},
  language = {en},
  timestamp = {2017-06-27T22:47:05Z},
  urldate = {2017-06-26},
  journal = {Psychonomic Bulletin \& Review},
  author = {Sch{\"o}nbrodt, Felix D. and Wagenmakers, Eric-Jan},
  month = mar,
  year = {2017},
  note = {00000},
  pages = {1--15}
}

